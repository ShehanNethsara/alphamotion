{
  "expo": {
    "name": "AlphaMotion",
    "slug": "alphamotion",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/gym.png",
    "scheme": "alphamotion",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/17.png",
        "backgroundImage": "./assets/images/18.png",
        "monochromeImage": "./assets/images/19.png"
      },
      "package": "com.shehannethsara.alphamotion",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "INTERNET"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/1.jpg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/17.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b6acb130-68e6-41b2-92e0-75b1347d8569"
      }
    }
  }
}